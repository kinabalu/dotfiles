" .vimrc - nothing special here.
syntax on
set ai
set nu
set ruler
set sts=2
set bs=2
set noic
set nowrap
set sw=2
set expandtab
set tabstop=2
set background=light
set history=500
set splitright
set hlsearch
set fo-=o
set fo-=c
set fo-=r

" Graphical Editor Dimensions (gedim) support:
let g:screenDimensions = { '1920,1080': [ 304, 80 ], '1440,900': [ 224, 68 ], '1280,768': [ 196, 56 ] }
